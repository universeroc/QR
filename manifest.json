{
  "background": {
    "page": "background.html"
  },
  "manifest_version": 2,

  "name": "QR",
  "description": "QR for current url.",
  "version": "1.0",

  "page_action": {
    "default_icon": "icon.png",
    "default_title": "QR",
    "default_popup": "qr.html"
  },
  "icons": {
    "128": "icon_128.png",
    "32": "icon_32.png",
    "48": "icon_48.png"
  },
  "permissions": [ "tabs", "https://*/*", "http://*/*" ]
}
